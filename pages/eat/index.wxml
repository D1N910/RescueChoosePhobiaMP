<!--pages/eat/index.wxml-->

<view
class="weui-cell"
wx:for="{{poisData}}"
wx:key="{{item.id}}">
      <view class="weui-cell__hd" data-text="{{item.name}}" style="position: relative;margin-right: 10px;">
          <image
            src="{{item.photos[0].url}}"
            style="width: 50px; height: 50px; display: block"/>
      </view>
      <view class="weui-cell__bd">
          <view>{{item.name}}</view>
          <view style="font-size: 13px;color: #888888;">{{item.address}}</view>
      </view>
  </view>